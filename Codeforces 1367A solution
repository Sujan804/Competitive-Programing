#include<bits/stdc++.h>
#define kaj_ses return 0
using namespace std;
int main()
{
    std::ios::sync_with_stdio(false);
    std::cin.tie(0);
    std::cout.tie(0);


    int t;
    cin>>t;
    while(t--)
    {
        string b;
        int i,j;
        cin>>b;
        int n = b.length();
        cout<<b[0];
        for(i=1;i<n-1;i+=2)
        {
          cout<<b[i];
        }
        cout<<b[n-1]<<endl;

    }
    kaj_ses;
}

